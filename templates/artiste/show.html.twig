{% extends 'base.html.twig' %}

{% block title %}Artiste{% endblock %}

{% block body %}
    <div class="container">
        <h1>Artiste</h1>
        <hr>

        <table class="table">
            <tbody>
            <tr>
                <th>Id</th>
                <td>{{ artiste.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ artiste.nom }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ artiste.pays }}</td>
            </tr>
            <tr>
                <th>Style</th>
                <td>{{ artiste.style }}</td>
            </tr>
            <tr>
                <th>Presentation</th>
                <td>{{ artiste.presentation }}</td>
            </tr>
            </tbody>
        </table>

        <a href="{{ path('artiste_index') }}">back to list</a>

        <a href="{{ path('artiste_edit', {'id': artiste.id}) }}">edit</a>

        {{ include('artiste/_delete_form.html.twig') }}
    </div>
{% endblock %}
